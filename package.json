{
  "name": "zod-endpoint",
  "version": "1.1.0",
  "description": "Lightweight zod and express integration enabling type-safe HTTP endpoints",
  "exports": {
    "./spec": {
      "import": "./dist/spec.mjs",
      "require": "./dist/spec.js"
    },
    "./server": {
      "import": "./dist/server.mjs",
      "require": "./dist/server.js"
    },
    "./client": {
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    }
  },
  "typesVersions": {
    "*": {
      "spec": [
        "dist/spec.d.ts"
      ],
      "server": [
        "dist/server.d.ts"
      ],
      "client": [
        "dist/client.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "pnpm run build:ts && pnpm run build:docs",
    "build:ts": "tsup",
    "build:docs": "typedoc --readme none src/server.ts src/client.ts src/spec.ts",
    "test": "jest"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "zod",
    "api",
    "express",
    "axios",
    "rest",
    "type-safety"
  ],
  "author": {
    "name": "Lorefnon",
    "email": "lorefnon@tutanota.com",
    "url": "https://lorefnon.me"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.2",
    "@types/lodash": "^4.14.182",
    "axios": "^0.27.2",
    "express": "^4.18.1",
    "jest": "^28.1.1",
    "ts-jest": "^28.0.5",
    "tsup": "^6.1.2",
    "typedoc": "^0.22.17",
    "typedoc-plugin-markdown": "^3.12.1",
    "typescript": "^4.7.4",
    "zod": "^3.17.3"
  },
  "peerDependencies": {
    "axios": "^0.27.2",
    "express": "^4.18.1",
    "zod": "^3.17.3"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "ts-pattern": "^4.0.5"
  }
}
